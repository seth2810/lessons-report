{
  "name": "lessons-report",
  "version": "0.0.1",
  "description": "ExpressJS based web server to work with lessons data",
  "main": "index.mjs",
  "type": "module",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "npx eslint . --no-error-on-unmatched-pattern",
    "test": "npx jest --bail --passWithNoTests --forceExit --setupFiles dotenv/config",
    "start": "node -r dotenv/config index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seth2810/lessons-report.git"
  },
  "keywords": [
    "lessons",
    "report"
  ],
  "author": "seth2810",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/seth2810/lessons-report/issues"
  },
  "homepage": "https://github.com/seth2810/lessons-report#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "babel-jest": "^27.0.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "http-codes": "^1.0.0",
    "jest": "^27.0.6",
    "supertest": "^6.1.6"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-joi-validation": "^5.0.0",
    "joi": "^17.4.2",
    "knex": "^0.21.21",
    "pg": "^8.7.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.m?jsx?$": "babel-jest"
    }
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  }
}
